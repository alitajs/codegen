  "basePath": "/",
  "tags": [{ "name": "fake_chart_data", "description": "图表数据" }],
  "paths": {
    "/": {},
    "/api/fake_chart_data": {
      "get": {
        "tags": ["fake_chart_data"],
        "summary": "图表数据",
        "description": "图表数据:假的图表数据，这里是对这个接口的说明",
        "operationId": "fakeChartDataPOST",
        "consumes": ["application/json"],
        "produces": ["application/json;charset=UTF-8"],
